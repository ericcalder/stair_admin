<!-- modal -->
<div class="modal fade" id="modal_new_customer_acct_form">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">Ã—</span><span class="sr-only">Close</span>
        </button>
        <h4 class="modal-title">Create New Customer Account</h4>
      </div>
      <div class="modal-body">
         <h4 id='propadd'></h4>

      </div>

      <form class='modal-body' id='new_cust_acct'>
        <label for='new_name'>Name:</label>
        <input type="text" name="name" id="new_name">
        <label for='new_email'>email:</label>
        <input type="text" name="email" id="new_email"><br>
        <label for='new_start_date'>Start Date:</label>
        <input type="text" name="start_date" id="new_start_date"><br>
        <label for='new_bal'>Balance:</label>
        <input type="number" step=0.01 id="new_bal">
        <label for='new_bal_date'>Balance Date:</label>
        <input type="text" id="new_bal_date">
        <label for='propID'>property_id:</label>
        <input type="number"  id="propID" readonly>
  
  
        <div>
          <input type="submit" name="g" value="Submit" id="g">
        </div>
     </form>
      
          <p id='error-message'></p>

      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" id="btnSave" class="btn btn-primary">Save changes</button>
      </div>
    </div>
    <!-- /.modal-content -->
  </div>
</div>

